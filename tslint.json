{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-no-circular-imports"
  ],
  "jsRules": { },
  "rules": {
    "interface-name": [false],
    "no-console": [false],
    "quotemark": [true, "single"],
    "arrow-parens": [false],
    "object-literal-sort-keys": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore"
      ]
    },
    "no-empty": [
      true,
      "allow-empty-catch"
    ]
  },
  "rulesDirectory": []
}
